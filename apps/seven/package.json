    "@vitest/coverage-v8": "catalog:",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "react-router-devtools": "^1.1.6",
    "release-it": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
