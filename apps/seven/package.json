  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@plone/types": "workspace:*",
    "@react-router/dev": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@testing-library/jest-dom": "^6.6.3",
