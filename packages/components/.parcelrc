{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "@parcel/transformer-js",
      "@parcel/transformer-react-refresh-wrap"
    ]
  },
  "optimizers": {
    "index.js": [
      "...",
      "parcel-optimizer-react-client"
    ],
    "index.cjs": [
      "...",
      "parcel-optimizer-react-client"
    ]
  }
}
