    "babel-plugin-react-intl": "5.1.17",
    "babel-preset-razzle": "4.2.17",
    "chalk": "4",
    "commander": "8.2.0",
    "comment-json": "^4.2.3",
    "execa": "0.6.3",
    "find-parent-dir": "^0.3.1",
    "fs-extra": "10.1.0",
    "git-url-parse": "^13.1.0",
    "glob": "7.1.6",
    "lodash": "4.17.21",
    "mrs-developer": "^2.1.1",
    "pofile": "1.0.10",
