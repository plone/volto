  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:robgietema/mosaic.git"
  },
  "homepage": "https://plone.org",
  "keywords": [
    "mosaic"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "better-npm-run start-prod",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others 'better-npm-run watch-client' 'better-npm-run start-dev'",
    "jsdoc": "jsdoc -c jsdoc.json; : 'ignore minor errors'",
    "pretest": "concurrent 'npm run lint' 'npm run jsdoc'",
    "test": "concurrent 'npm run test:src:unit' 'npm run test:src:e2e'",
    "pretest:e2e": "webdriver-manager update",
    "test:e2e": "protractor ./protractor.conf.js",
    "test:unit": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
